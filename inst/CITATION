citHeader("To cite RcppAlgos in publications use:")

citEntry(
  entry  = "Manual",
  title  = "RcppAlgos: High Performance Tools for Combinatorics and Computational Mathematics",
  author = person("Joseph", "Wood"),
  year   = format(Sys.Date(), "%Y"),
  note   = paste("RcppAlgos package version", packageVersion("RcppAlgos")),
  url    = "https://jwood000.github.io/RcppAlgos",
  textVersion = paste0(
    "Wood J (", format(Sys.Date(), "%Y"),
    "). RcppAlgos: High Performance Tools for Combinatorics and Computational Mathematics. ",
    "RcppAlgos package version ", packageVersion("RcppAlgos"),
    ". https://jwood000.github.io/RcppAlgos"
  )
)
